<!DOCTYPE html>
<html>
    <head>
        <title>@Page.Title</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/common.css" />
        <script src="/addStylsheet.js"></script>
        @RenderSection("head", required: false)
    </head>
    <body>
        <header>
            <div class="headerSection scroll" style="background-color: white;">
                <div class="headerPush">
                    <a href="/index.cshtml">
                        <div class="headerWrap">
                            <div class="logo"></div>
                            <div class="title">Falkarna Syd</div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="promotionBtn float-right scroll" id="promoPlaceholder">
            </div>
            <div class="headerSection scroll float-right" style="background-color: rgb(250,250,250);">
                <div class="headerPush">
                    <nav>
                        <a href="#"><div class="menuBtn">Diskussion</div></a>
                        <a href="http://falkarna.quickbutik.com" target="_blank"><div class="menuBtn">Webbshop</div></a>
                    </nav>
                </div>
            </div>
        </header>
        <a href="https://apply.cardskipper.se/eid" target="_blank">
            <div class="promotionBtn real scroll">
                <div class="promotionWrap">
                    Gå med idag!
                </div>
            </div>
        </a>
        <div class="backPlaceholder"></div>
        <div class="background" style="background-image: linear-gradient(rgba(0,0,0,0.25),rgba(0,0,0,0.05)), url(Resources/Common/Background/back1.jpg); z-index: 0;"></div>
        <div class="background" style="background-image: linear-gradient(rgba(0,0,0,0.25),rgba(0,0,0,0.05)), url(Resources/Common/Background/back2.jpg); z-index: -1;"></div>
        <div class="background" style="background-image: linear-gradient(rgba(0,0,0,0.25),rgba(0,0,0,0.05)), url(Resources/Common/Background/back3.jpg); z-index: -2;"></div>
        <div id="mediaCanvasCont">
            <div id="mediaCanvasWrap">
                <div id="mediaCanvasPush">
                    <div id="mediaCanvas" class="imgCanvas">
                        <div class="navBtn next" onclick="nextImg(0)">&#9658;</div>
                        <div class="navBtn prev" onclick="prevImg(0)">&#9668;</div>
                        <div class="closeBtn" onclick="closeCanvas()">&times;</div>
                        <div class="imgIdx">1/11</div>
                    </div>
                </div>
            </div>
        </div>
        <main>
            @RenderBody()
        </main>
        <footer>
            <div class="pageWidth fullHeight">
                <div class="footerPush">
                    <div class="footerContentWrap">
                        <div class="footerTitle">
                            <a href="/index.cshtml">
                                <div class="footerLogo"></div>
                                <!--<div class="title">Falkarna Syd</div>-->
                            </a>
                        </div>
                        <div class="footerPush">
                            <table class="contactInfo float-right">
                                <tr>
                                    <td>E-Postadress</td>
                                    <td>falkarna@falkarna-ofk.se</td>
                                </tr>
                                <tr>
                                    <td>Medlemskap</td>
                                    <td>123 287 67 79</td>
                                </tr>
                                <tr>
                                    <td>Försäljning</td>
                                    <td>123 274 01 40</td>
                                </tr>
                                <tr>
                                    <td>Tifo och övriga bidrag</td>
                                    <td>123 560 02 67</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </body>
</html>
